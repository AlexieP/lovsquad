    <!DOCTYPE html>
      <html>
      <head>
        <title>Instafeed on Your Website</title>
        <style type="text/css">
          a img{
            width: 25%;
            gap: 16px;
            padding: 10px;
            margin: 10px;
          }
        </style>
      </head>
      <body>
        <h1 style="text-align: center">Instagram Feed</h1>
            <div id="instafeed-container"></div>



        <script src="https://cdn.jsdelivr.net/gh/stevenschobert/instafeed.js@2.0.0rc1/src/instafeed.min.js"></script>
        <script type="text/javascript">
        var userFeed = new Instafeed({
          get: 'user',
          target: "instafeed-container",
          resolution: 'low_resolution',
          accessToken: "<%= session && session[:access_token] %>"
        });
        userFeed.run();
        </script>
      </body>
      </html>

    <div>
    <% if @username %>
      <p>here is your username: <strong><%= @username %></strong></p>
    <% end %>
    <a href="https://api.instagram.com/oauth/authorize?client_id=425954065799626&redirect_uri=<%=ENV['INSTAGRAM_REDIRECT_URI']%>/code&scope=user_profile,user_media&response_type=code">FB</a>
    </div>
